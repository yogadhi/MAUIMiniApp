<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:main="clr-namespace:MAUIMiniApp"
             x:Class="MAUIMiniApp.AppTabbed"
             Title="AppTabbed">
    <NavigationPage Title="One" IconImageSource="{StaticResource TabOne}">
        <x:Arguments>
            <main:MainPage Title="One" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Two" IconImageSource="{StaticResource TabTwo}">
        <x:Arguments>
            <main:MainPage Title="Two" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Three" IconImageSource="{StaticResource TabThree}">
        <x:Arguments>
            <main:MainPage Title="Three" />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Four" IconImageSource="{StaticResource TabFour}">
        <x:Arguments>
            <main:MainPage Title="Four" />
        </x:Arguments>
    </NavigationPage>

</TabbedPage>